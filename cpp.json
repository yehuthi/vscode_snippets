{
	"Extern C": {
		"description": "extern C",
		"prefix": "externc",
		"body": [
			"#ifdef __cplusplus",
			"extern \"C\" {",
			"#endif",
			"",
			"$0",
			"",
			"#ifdef __cplusplus",
			"}",
			"#endif",
		]
	},

	"Pragma Once Extern C": {
		"description": "#pragma once + extern C",
		"prefix": "pragma_c",
		"body": [
			"#pragma once",
			"#ifdef __cplusplus",
			"extern \"C\" {",
			"#endif",
			"",
			"$0",
			"",
			"#ifdef __cplusplus",
			"}",
			"#endif",
		]
	}
}
